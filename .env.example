# Database
DATABASE_URL=
PRISMA_LOCAL=

# Better Auth Configuration
BETTER_AUTH_SECRET=
BETTER_AUTH_URL=

# Allowed User Email (REQUIRED - only this email can access the app)
ALLOWED_EMAIL=

# Google OAuth (Get credentials at: https://console.cloud.google.com/apis/credentials)
# 1. Create a new OAuth 2.0 Client ID
# 2. Add authorized redirect URI: http://localhost:3000/api/auth/callback/google
# 3. For production, also add: https://yourdomain.com/api/auth/callback/google
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# GitHub OAuth (Get credentials at: https://github.com/settings/developers)
# 1. Create a new OAuth App
# 2. Set authorization callback URL: http://localhost:3000/api/auth/callback/github
# 3. For production, also add: https://yourdomain.com/api/auth/callback/github
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=

# Plaid API (Financial Data)
PLAID_CLIENT_ID=
PLAID_SECRET=
PLAID_ENV=
PLAID_PRODUCTS=auth,transactions,investments
PLAID_COUNTRY_CODES=US,CA

# Alpha Vantage API for stock prices (Get free key at: https://www.alphavantage.co/support/#api-key)
ALPHA_VANTAGE_API_KEY=

# OpenAI API (For transaction categorization)
OPENAI_API_KEY=
